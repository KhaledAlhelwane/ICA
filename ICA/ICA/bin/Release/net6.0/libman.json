{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-rtl@3.3.4",
      "destination": "wwwroot/bootstrap-rtl/"
    },
    {
      "library": "bootstrap-v4-rtl@4.6.2-1",
      "destination": "wwwroot/lib/bootstrap-v4-rtl/",
      "files": [
        "js/bootstrap.min.js",
        "css/bootstrap-rtl.css"
      ]
    },
    {
      "library": "summernote@0.8.20",
      "destination": "wwwroot/lib/summernote/summernote/",
